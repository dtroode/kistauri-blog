{
  "name": "kistauri",
  "description": "Назови любое слово и я сделаю об этом сайт.",
  "author": "Давид Кистаури <davidkis113@gmail.com>",
  "keywords": [
    "gatsby",
    "portfolio",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop --open",
    "lint": "eslint src/ && stylelint src/styles/**/*.scss",
    "spell": "yaspeller-ci \"src/**/**.md\"",
    "test": "npm run lint && npm run spell",
    "clean": "rm -rf dist/ .cache",
    "build": "npm run lint && gatsby build"
  },
  "version": "2.1.0",
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dtroode/kistauri"
  },
  "bugs": {
    "url": "https://github.com/dtroode/kistauri/issues"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "gatsby": "^2.19.19",
    "gatsby-image": "^2.1.4",
    "gatsby-plugin-feed": "^2.2.3",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-preact": "^3.1.13",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.5",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-remark-images": "^3.0.16",
    "gatsby-remark-prismjs": "^3.3.1",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-source-filesystem": "^2.0.39",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-sharp": "^2.1.21",
    "node-sass": "^4.12.0",
    "preact": "^10.2.1",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "r": "0.0.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "sharp": "^0.22.1",
    "typescript": "^3.6.2",
    "typograf": "^6.8.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-preset-gatsby": "^0.2.8",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "gatsby-plugin-sass": "^2.1.29",
    "identity-obj-proxy": "^3.0.0",
    "sass": "^1.21.0",
    "stylelint": "^13.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.0.0",
    "yaspeller-ci": "^1.0.2"
  },
  "browserslist": [
    "> 0.25%",
    "not dead",
    "last 2 chrome version",
    "last 2 safari version",
    "last 2 firefox version",
    "last 2 opera version",
    "last 2 edge version",
    "last 2 ios_saf version",
    "last 2 and_chr version",
    "last 2 and_ff version",
    "last 2 and_uc version"
  ],
  "stylelint": {
    "plugins": [
      "stylelint-order"
    ],
    "extends": [
      "stylelint-config-rational-order"
    ],
    "rules": {
      "plugin/rational-order": [
        true,
        {
          "empty-line-between-groups": true
        }
      ],
      "color-named": "never",
      "order/order": [
        "declarations",
        {
          "type": "at-rule",
          "name": "media"
        },
        {
          "type": "rule",
          "selector": "^&::(before|after)"
        },
        {
          "type": "rule",
          "selector": "^&:\\w"
        },
        {
          "type": "rule",
          "selector": "^&_"
        },
        {
          "type": "rule",
          "selector": "^."
        }
      ]
    }
  }
}
