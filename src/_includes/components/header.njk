<header class="head">
  <img src="/images/profile.jpg" height="36.8px" width="36.8px">
  <nav class="head__nav" aria-label="Страницы сайта">
    {%- for entry in collections.all | eleventyNavigation -%}
      <a class="head__a a--primary {% if entry.url == page.url %} a--active {% endif %}{% if entry.key == eleventyNavigation.parent or (entry.url in page.url and entry.url != page.url and entry.key != 'kistauri') %} a--parent-active{% endif %}" href="{{ entry.url | url }}">{{ entry.title }}</a>
    {%- endfor -%}
    {%- if pageClass == "blog" or pageClass == "tag" or pageClass == "notes" -%}
      <a class="head__a a--secondary" href="/blog/tags/">Теги</a>
    {%- endif -%}
    {%- if pageClass == "post" and projectURL -%}
      <a class="head__a a--secondary" href="https://{{projectURL}}">{{ projectURL }}</a>
    {%- endif -%}
  </nav>
  <h1 class="head__header head__header--{{pageClass}} {% if eleventyNavigation.parent %} head__header--subsection {% endif %}">{{ renderData.title or title or metadata.title }}</h1>
  {# <hr> #}
</header>
